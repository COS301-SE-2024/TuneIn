  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@playwright/test": "^1.47.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/react": "~18.2.45",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-native": "^0.73.0",
    "@types/react-native-svg-charts": "^5.0.16",
