    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "prettier": "^3.3.0",
    "react-native-dotenv": "^3.4.11",
    "typescript": "~5.3.3"
  },
  "private": true,
  "jest": {
    "setupFiles": [
      "./jest.setup.ts"
    ],
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-navigation|expo(nent)?|@expo(nent)?/.*|react-clone-referenced-element|@unimodules/.*|unimodules|sentry-expo|native-base|@smithy/middleware-retry|@aws-sdk/client-s3|@aws-sdk/lib-storage|uuid)"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "text-summary",
      "json-summary",
      "lcov"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/api-client/**",
      "!**/e2e/**"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/api-client/",
      "/e2e/"
    ]
  },
  "engines": {
    "node": "18.18.2",
    "npm": "9.8.1"
  }
