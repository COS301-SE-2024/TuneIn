{
  "name": "tunein",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scheme": "tunein-app",
  "scripts": {
    "prestart": "node check-versions.js && node node_modules/react-native-localhost/scripts/postinstall.js",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint-fix": "eslint --fix .",
    "lint": "eslint .",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.3",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "^8.1.1",
    "@react-native-community/slider": "^4.5.2",
    "@testing-library/react-native": "^12.5.1",
    "@types/react-native-dotenv": "^0.2.2",
    "amazon-cognito-identity-js": "^6.3.12",
    "aws-sdk": "^2.1646.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "expo": "~51.0.9",
    "expo-auth-session": "~5.5.2",
    "expo-av": "^14.0.5",
    "expo-crypto": "~13.0.2",
    "expo-image-picker": "^15.0.5",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "^3.5.15",
    "expo-status-bar": "~1.12.1",
    "expo-web-browser": "~13.0.3",
    "isomorphic-fetch": "^3.0.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-elements": "^3.4.3",
    "react-native-localhost": "^1.0.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "^3.12.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-snap-carousel": "^1.6.1",
    "react-native-svg": "^15.2.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-web": "~0.19.10",
    "socket.io-client": "^4.7.5",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.2",
    "prettier": "^3.3.0",
    "react-native-dotenv": "^3.4.11",
    "typescript": "^5.1.3"
  },
  "private": true,
  "jest": {
    "setupFiles": [
      "./jest.setup.ts"
    ],
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-navigation|expo(nent)?|@expo(nent)?/.*|react-clone-referenced-element|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "text-summary",
      "json-summary",
      "lcov"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/api-client/**"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/api-client/"
    ]
  },
  "engines": {
    "node": "18.18.2",
    "npm": "9.8.1"
  }
}
