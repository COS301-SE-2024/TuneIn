{
  "name": "tunein",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scheme": "tunein-app",
  "scripts": {
    "prestart": "node check-versions.js && node node_modules/react-native-localhost/scripts/postinstall.js",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint-fix": "eslint --fix .",
    "lint": "eslint .",
    "test": "jest",
    "test:cov": "jest --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "dependencies": {
